        <mxCell id="b_Z4Wu49CTBc0JAxHN4Z-34" value="&lt;font style=&quot;font-size: 16px;&quot;&gt;Random&lt;/font&gt;" style="whiteSpace=wrap;html=1;aspect=fixed;fontFamily=CaskaydiaCove Nerd Font;" vertex="1" parent="1">
