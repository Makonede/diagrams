        <mxCell id="b_Z4Wu49CTBc0JAxHN4Z-16" value="&lt;font style=&quot;font-size: 16px;&quot;&gt;add x1, x1, w9, SXTW&lt;/font&gt;" style="whiteSpace=wrap;html=1;aspect=fixed;fontFamily=CaskaydiaCove Nerd Font;" vertex="1" parent="1">
