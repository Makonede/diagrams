        <mxCell id="b_Z4Wu49CTBc0JAxHN4Z-37" value="" style="endArrow=none;dashed=1;html=1;strokeWidth=8;rounded=0;dashPattern=1 4;" edge="1" parent="1">
          <mxGeometry width="50" height="50" relative="1" as="geometry">
            <mxPoint x="520" y="399.66" as="sourcePoint" />
            <mxPoint x="840" y="399.66" as="targetPoint" />
